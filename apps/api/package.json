{
  "name": "@pmo/api",
  "version": "1.0.0",
  "private": true,
  "description": "PMO API Server",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "test:api": "node test-api.js",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.914.0",
    "@aws-sdk/credential-providers": "^3.915.0",
    "@aws-sdk/s3-request-presigner": "^3.914.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/swagger": "^9.3.0",
    "@fastify/swagger-ui": "^5.0.1",
    "@fastify/type-provider-typebox": "^5.0.0",
    "@fastify/websocket": "^11.2.0",
    "@sinclair/typebox": "^0.31.28",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/minio": "^7.1.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.29.4",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "lib0": "^0.2.114",
    "minio": "^8.0.6",
    "openai": "^6.8.1",
    "pg": "^8.11.3",
    "pino-pretty": "^13.1.1",
    "postgres": "^3.4.7",
    "redis": "^4.6.12",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.27",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "drizzle-kit": "^0.20.10",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}