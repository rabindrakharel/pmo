      "Bash(PGPASSWORD=app psql -h localhost -p 5434 -U app -d app -c \"\n-- Create tasks for Fall 2025 Landscaping Campaign\nINSERT INTO app.ops_task_head (\n  name, descr, tags, attr, \n  project_id, task_code, priority_level, task_type, \n  status, planned_start_date, planned_end_date, \n  estimated_hours, actual_hours\n) VALUES\n-- Site Preparation Tasks\n(''Site Survey and Assessment'', ''Comprehensive site evaluation including soil testing, drainage assessment, and existing landscape analysis for optimal fall landscaping preparation'', \n ''[\"\"site_prep\"\", \"\"assessment\"\", \"\"soil_testing\"\", \"\"drainage\"\", \"\"landscaping\"\"]'', \n ''{\"\"task_category\"\": \"\"preparation\"\", \"\"equipment_needed\"\": [\"\"soil_testing_kit\"\", \"\"measuring_tools\"\", \"\"camera\"\"], \"\"safety_requirements\"\": [\"\"safety_vest\"\", \"\"hard_hat\"\"], \"\"weather_dependent\"\": true, \"\"client_coordination\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-001'', ''high'', ''field_work'',\n ''planned'', ''2025-09-01 08:00:00'', ''2025-09-03 17:00:00'', 16, 0),\n\n(''Property Boundary Marking'', ''Mark property boundaries and establish work zones for landscaping activities, ensuring compliance with municipal requirements'',\n ''[\"\"boundaries\"\", \"\"marking\"\", \"\"compliance\"\", \"\"zoning\"\", \"\"municipal\"\"]'',\n ''{\"\"task_category\"\": \"\"preparation\"\", \"\"equipment_needed\"\": [\"\"surveying_equipment\"\", \"\"boundary_markers\"\", \"\"measuring_tape\"\"], \"\"permits_required\"\": true, \"\"municipal_compliance\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-002'', ''high'', ''field_work'',\n ''planned'', ''2025-09-04 08:00:00'', ''2025-09-05 17:00:00'', 12, 0),\n\n-- Equipment and Material Procurement\n(''Equipment Procurement and Setup'', ''Acquire and set up all necessary landscaping equipment including excavators, aerators, seeders, and hand tools'',\n ''[\"\"equipment\"\", \"\"procurement\"\", \"\"setup\"\", \"\"tools\"\", \"\"machinery\"\"]'',\n ''{\"\"task_category\"\": \"\"logistics\"\", \"\"equipment_list\"\": [\"\"excavator\"\", \"\"aerator\"\", \"\"seeder\"\", \"\"rakes\"\", \"\"shovels\"\", \"\"wheelbarrows\"\"], \"\"rental_coordination\"\": true, \"\"maintenance_check\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-003'', ''medium'', ''logistics'',\n ''planned'', ''2025-08-28 09:00:00'', ''2025-08-30 17:00:00'', 20, 0),\n\n(''Material Delivery Coordination'', ''Coordinate delivery of soil amendments, mulch, fertilizers, and seasonal plants for fall installation'',\n ''[\"\"materials\"\", \"\"delivery\"\", \"\"coordination\"\", \"\"soil\"\", \"\"plants\"\", \"\"mulch\"\"]'',\n ''{\"\"task_category\"\": \"\"logistics\"\", \"\"materials_list\"\": [\"\"topsoil\"\", \"\"compost\"\", \"\"fall_fertilizer\"\", \"\"mulch\"\", \"\"seasonal_plants\"\", \"\"bulbs\"\"], \"\"supplier_coordination\"\": true, \"\"quality_inspection\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-004'', ''medium'', ''logistics'',\n ''planned'', ''2025-09-06 08:00:00'', ''2025-09-08 17:00:00'', 18, 0),\n\n-- Core Landscaping Tasks\n(''Soil Preparation and Amendment'', ''Prepare soil through aeration, amendment addition, and pH balancing for optimal fall planting conditions'',\n ''[\"\"soil\"\", \"\"preparation\"\", \"\"aeration\"\", \"\"amendment\"\", \"\"ph_balancing\"\"]'',\n ''{\"\"task_category\"\": \"\"soil_work\"\", \"\"processes\"\": [\"\"soil_testing\"\", \"\"aeration\"\", \"\"amendment_application\"\", \"\"ph_adjustment\"\"], \"\"weather_dependent\"\": true, \"\"quality_standards\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-005'', ''high'', ''field_work'',\n ''planned'', ''2025-09-09 08:00:00'', ''2025-09-12 17:00:00'', 28, 0),\n\n(''Fall Seeding and Overseeding'', ''Apply grass seed and overseed existing lawn areas with cool-season varieties optimal for fall establishment'',\n ''[\"\"seeding\"\", \"\"overseeding\"\", \"\"grass\"\", \"\"cool_season\"\", \"\"establishment\"\"]'',\n ''{\"\"task_category\"\": \"\"planting\"\", \"\"seed_types\"\": [\"\"tall_fescue\"\", \"\"perennial_ryegrass\"\", \"\"fine_fescue\"\"], \"\"application_method\"\": \"\"broadcast_seeding\"\", \"\"weather_dependent\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-006'', ''high'', ''field_work'',\n ''planned'', ''2025-09-13 08:00:00'', ''2025-09-16 17:00:00'', 24, 0),\n\n(''Seasonal Plant Installation'', ''Install fall flowering plants, bulbs, and seasonal decorative elements for autumn and spring displays'',\n ''[\"\"planting\"\", \"\"bulbs\"\", \"\"seasonal\"\", \"\"flowers\"\", \"\"decorative\"\"]'',\n ''{\"\"task_category\"\": \"\"planting\"\", \"\"plant_types\"\": [\"\"mums\"\", \"\"asters\"\", \"\"spring_bulbs\"\", \"\"ornamental_kale\"\"], \"\"design_coordination\"\": true, \"\"client_preferences\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-007'', ''medium'', ''field_work'',\n ''planned'', ''2025-09-17 08:00:00'', ''2025-09-20 17:00:00'', 32, 0),\n\n-- Maintenance and Care Tasks\n(''Fertilizer Application Program'', ''Apply fall-specific fertilizer program to promote root development and winter hardiness'',\n ''[\"\"fertilizer\"\", \"\"application\"\", \"\"root_development\"\", \"\"winter_prep\"\", \"\"nutrition\"\"]'',\n ''{\"\"task_category\"\": \"\"maintenance\"\", \"\"fertilizer_types\"\": [\"\"fall_blend\"\", \"\"root_stimulator\"\", \"\"potassium_boost\"\"], \"\"application_schedule\"\": \"\"staged_approach\"\", \"\"weather_monitoring\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-008'', ''medium'', ''field_work'',\n ''planned'', ''2025-09-21 08:00:00'', ''2025-09-23 17:00:00'', 16, 0),\n\n(''Mulching and Ground Cover'', ''Apply mulch and establish ground cover in landscaped areas for winter protection and weed suppression'',\n ''[\"\"mulching\"\", \"\"ground_cover\"\", \"\"winter_protection\"\", \"\"weed_suppression\"\"]'',\n ''{\"\"task_category\"\": \"\"maintenance\"\", \"\"mulch_types\"\": [\"\"shredded_hardwood\"\", \"\"leaf_mold\"\", \"\"pine_needles\"\"], \"\"coverage_depth\"\": \"\"3_inches\"\", \"\"edge_definition\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-009'', ''medium'', ''field_work'',\n ''planned'', ''2025-09-24 08:00:00'', ''2025-09-27 17:00:00'', 28, 0),\n\n-- Quality Control and Documentation\n(''Quality Inspection and Documentation'', ''Conduct comprehensive quality inspection and document all work completed with photo documentation'',\n ''[\"\"quality_control\"\", \"\"inspection\"\", \"\"documentation\"\", \"\"photography\"\", \"\"standards\"\"]'',\n ''{\"\"task_category\"\": \"\"quality_assurance\"\", \"\"inspection_areas\"\": [\"\"seeding_coverage\"\", \"\"plant_health\"\", \"\"edge_definition\"\", \"\"cleanup\"\"], \"\"documentation_required\"\": true, \"\"client_walkthrough\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-010'', ''high'', ''administrative'',\n ''planned'', ''2025-09-28 08:00:00'', ''2025-09-30 17:00:00'', 12, 0),\n\n-- Project Management Tasks\n(''Client Communication and Updates'', ''Maintain regular client communication with progress updates, schedule adjustments, and completion notifications'',\n ''[\"\"client_communication\"\", \"\"updates\"\", \"\"progress\"\", \"\"schedule\"\", \"\"notifications\"\"]'',\n ''{\"\"task_category\"\": \"\"communication\"\", \"\"communication_schedule\"\": \"\"weekly_updates\"\", \"\"methods\"\": [\"\"email\"\", \"\"phone\"\", \"\"site_visits\"\"], \"\"documentation\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-011'', ''medium'', ''administrative'',\n ''planned'', ''2025-09-01 09:00:00'', ''2025-09-30 17:00:00'', 8, 0),\n\n(''Weather Monitoring and Schedule Adjustment'', ''Monitor weather conditions and adjust work schedule to optimize results and avoid weather-related delays'',\n ''[\"\"weather_monitoring\"\", \"\"schedule_adjustment\"\", \"\"optimization\"\", \"\"planning\"\"]'',\n ''{\"\"task_category\"\": \"\"project_management\"\", \"\"monitoring_tools\"\": [\"\"weather_apps\"\", \"\"soil_temperature\"\"], \"\"adjustment_criteria\"\": [\"\"precipitation\"\", \"\"temperature\"\", \"\"wind\"\"], \"\"contingency_planning\"\": true}'',\n ''93106ffb-402e-43a7-8b26-5287e37a1b0e'', ''FALL-2025-LAND-012'', ''high'', ''administrative'',\n ''planned'', ''2025-09-01 07:00:00'', ''2025-09-30 19:00:00'', 6, 0);\")",
